<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="TestNGLintenerSuite">
    <listeners>
        <listener class-name="listeners.Linteners"/>
    </listeners>

    <test name="LoginTest">
        <classes>
            <class name="listeners.LoginTest">
                <methods>
                    <include name="loginByEmail"/>
                    <include name="loginByFacebook"/>
                </methods>
            </class>
        </classes>
    </test>

</suite>